				<includes>
					<include>**/*.properties</include>
					<include>**/*.gif</include>
					<include>**/version</include>
				</includes>
				<excludes>
					<exclude>**/*.jar</exclude>
				</excludes>
				<filtering>false</filtering>
			</resource>
