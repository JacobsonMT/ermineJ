<?xml version="1.0"?>
<project 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://maven.apache.org/xsd/ http://maven.apache.org/xsd/maven-project-3.0.0.xsd">
    <pomVersion>3</pomVersion>
    <id>ermineJ</id>
    <name>ermineJ</name>
    <groupId>ermineJ</groupId>
    <currentVersion>2.0b3</currentVersion>
    <organization>
        <name>Columbia University</name>
        <url>http://www.columbia.edu</url>
        <logo>src/ermineJ/gui/resources/logo1small.gif</logo>
    </organization>
    <inceptionYear>2004</inceptionYear>
    <logo>src/classScore/gui/resources/logo1small.gif</logo>
    <description>Software for gene set analysis.</description>
    <shortDescription>Software for gene set analysis.</shortDescription>
    <url>http://microarray.cu-genome.org/ermineJ</url>
    <siteAddress>microarray</siteAddress>
    <siteDirectory>/proj/pavlidis/protected/ermineJ</siteDirectory>
    <distributionSite>microarray</distributionSite>
    <distributionDirectory>/proj/projectpages/ermineJ</distributionDirectory>
    <repository>
        <connection>scm:cvs:ext:${maven.username}@adgate.cu-genome.org:/fsnode/projects/cvs/pavlab:ermineJ</connection>
    </repository>

    <developers>
        <developer>
            <name>Paul Pavlidis</name>
            <id>pavlidis</id>
            <email>pavlidis@dbmi.columbia.edu</email>
            <organization>Columbia University</organization>
            <roles/>
        </developer>
        <developer>
            <name>William Braynen</name>
            <id>wbraynen</id>
            <email>wbraynen@ic.sunysb.edu</email>
            <organization>Columbia University</organization>
            <roles/>
        </developer>
        <developer>
            <name>Homin Lee</name>
            <id>hkl7</id>
            <email>hkl7@cs.columbia.edu</email>
            <organization>Columbia University</organization>
            <roles/>
        </developer>
    </developers>
    <licenses>
        <license><name>GPL</name></license>
    </licenses>
    <dependencies>
        <dependency>
            <groupId>colt</groupId>
            <artifactId>colt</artifactId>
                   <version/>
            <jar>colt.jar</jar>
     
            <type>jar</type>
            <url>http://hoschek.home.cern.ch/hoschek/colt/V1.0.3/download</url>
            <properties>
                <category>lib</category>
                <jnlp.jar>true</jnlp.jar>
            </properties>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>1.0.3</version>
            <type>jar</type>
            <properties>
                <category>lib</category>
                <jnlp.jar>true</jnlp.jar>
            </properties>
        </dependency>
        <dependency>
            <groupId>baseCode</groupId>
            <artifactId>baseCode</artifactId>
            <version>0.2.1</version>
            <type>jar</type>
            <properties>
                <category>lib</category>
                <jnlp.jar>true</jnlp.jar>
            </properties>
        </dependency>
        <dependency>
            <groupId>xerces</groupId>
            <artifactId>xercesImpl</artifactId>
            <version>2.6.2</version>
            <type>jar</type>
            <properties>
                <category>lib</category>
                <jnlp.jar>true</jnlp.jar>
            </properties>
        </dependency>
        <dependency>
            <groupId>javahelp</groupId>
            <artifactId>jhelp</artifactId>
            <version>2.0</version>
            <jar>jh.jar</jar>
            <type>jar</type>
            <properties/>
        </dependency>
    </dependencies>
    <build>
        <sourceDirectory>src</sourceDirectory>
        <unitTestSourceDirectory>test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/Test*.java</include>
            </includes>
        </unitTest>
        <resources>
            <resource>
                <directory>${basedir}/src/classScore/gui/resources</directory>
                <includes>
                    <include>*.gif</include>
                </includes>
                <filtering>false</filtering>
            </resource>
        </resources>
    </build>
    <reports>
        <report>maven-jdepend-plugin</report>
        <report>maven-pmd-plugin</report>
        <report>maven-changelog-plugin</report>
        <report>maven-file-activity-plugin</report>
        <report>maven-developer-activity-plugin</report>
        <report>maven-javadoc-plugin</report>
        <report>maven-jxr-plugin</report>
        <report>maven-junit-report-plugin</report>
        <report>maven-tasklist-plugin</report>
     <!--   <report>maven-clover-plugin</report> -->
    </reports>
    <properties/>
</project>