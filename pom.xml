<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>ermineJ</artifactId>
	<name>ermineJ</name>
	<groupId>ermineJ</groupId>
	<version>2.1.16</version>
	<organization>
		<name>UBC Centre for High-Throughput Biology</name>
		<url>http://bioinformatics.ubc.ca</url>
	</organization>
	<inceptionYear>2004</inceptionYear>
	<description>
		ErmineJ performs analyses of gene sets in expression microarray data. A typical goal is to determine whether
		particular biological pathways are "doing something interesting" in the data. The software is designed to be
		used by biologists with little or no informatics background.
	</description>
	<packaging>pom</packaging>
	<scm>
		<connection>scm:cvs:ext:@broca.ubic.ca:local:/cvs:ermineJ</connection>
		<developerConnection>scm:cvs:ext:@broca.ubic.ca:/cvs:ermineJ</developerConnection>
		<url>http://broca.ubic.ca:8081/viewrep/pavlab/ermineJ</url>
	</scm>
	<issueManagement>
		<system>Bugzilla</system>
		<url>
			http://www.bioinformatics.ubc.ca/pavlidis/lab/bugzilla/buglist.cgi?product=ermineJ&amp;bug_status=__open__
		</url>
	</issueManagement>
	<ciManagement>
		<system>continuum</system>
		<url>http://broca.ubic.ca/continuum/</url>
		<notifiers>
			<notifier>
				<type>mail</type>
				<sendOnError>true</sendOnError>
				<sendOnFailure>true</sendOnFailure>
				<sendOnSuccess>true</sendOnSuccess>
				<sendOnWarning>true</sendOnWarning>
				<configuration>
					<address>paul@bioinformatics.ubc.ca</address>
				</configuration>
			</notifier>
		</notifiers>
	</ciManagement>
	<licenses>
